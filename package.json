{
  "name": "react-filter-menu",
  "version": "0.1.5",
  "description": "A filter component for JSON data, utilising React and styled-components",
  "homepage": "https://github.com/chrismphilp/filter-menu",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrismphilp/filter-menu"
  },
  "bugs": {
    "url": "https://github.com/chrismphilp/filter-menu/issues"
  },
  "author": "Christopher Philp",
  "keywords": [
    "react",
    "react-component",
    "component",
    "filter"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "eslint --max-warnings 0 --ext .js \"src/**/*.{ts,tsx}\" --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^4.4.1",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "12.7.8",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-fbjs": "^3.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0"
  }
}
